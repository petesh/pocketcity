	$(CC) $(CFLAGS) -c $< -o $@

$(OBJDIR)/stack.o: stack.c stack.h
	$(CC) $(CFLAGS) -c $< -o $@

$(OBJDIR)/stringsearch.o: stringsearch.c stringsearch.h
	$(CC) $(CFLAGS) -c $< -o $@

